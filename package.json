{
	"name": "@flossbank/cli",
	"version": "0.0.17",
	"license": "MIT",
	"bin": {
		"flossbank": "bin.js"
	},
	"preferGlobal": true,
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"postinstall": "flossbank install || exit 0",
		"postuninstall": "node bin.js uninstall",
		"prepare": "npm t && npm run build",
		"build": "webpack",
		"test": "standard && nyc ava --match=\"!*integ*\"",
		"integ-tests": "ava --match=\"*integ*\""
	},
	"dependencies": {
		"update-notifier": "^4.0.0"
	},
	"devDependencies": {
		"abort-controller": "^3.0.0",
		"ava": "^2.4.0",
		"boxen": "^4.1.0",
		"chalk": "^2.4.2",
		"ci-info": "^2.0.0",
		"conf": "^6.2.0",
		"debug": "^4.1.1",
		"diffy": "^2.1.0",
		"husky": "^3.0.5",
		"minimist": "^1.2.0",
		"nock": "^11.7.2",
		"node-fetch": "^2.6.0",
		"nyc": "^14.1.1",
		"prompts": "^2.2.1",
		"sinon": "^7.5.0",
		"standard": "^13.1.0",
		"temp-write": "^4.0.0",
		"term-size": "^2.1.0",
		"terser-webpack-plugin": "^2.3.2",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10",
		"word-wrap": "^1.2.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/flossbank/cli.git"
	}
}
