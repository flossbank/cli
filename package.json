{
	"name": "@flossbank/cli",
	"version": "0.0.19",
	"license": "MIT",
	"bin": {
		"flossbank": "bin.js"
	},
	"preferGlobal": true,
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"postinstall": "node bin.js install || exit 0",
		"postuninstall": "node bin.js uninstall",
		"prepare": "npm run test:unit && npm run build",
		"build": "webpack",
		"test": "standard && npm run test:unit",
		"test:unit": "ava -v --timeout=15s --match=\"!*integ*\"",
		"test:unit:tap": "ava -vt --timeout=15s --match=\"!*integ*\"",
		"test:integ": "cd ci/tests && ava -v --timeout=1m --serial --match=\"*integ*\" && cd ../../",
		"test:integ:tap": "cd ci/tests && ava -vt --timeout=1m --serial --match=\"*integ*\" && cd ../../",
		"test:tap": "multi-tap -r test:unit:tap -r test:integ:tap | tap-xunit",
		"coverage": "cross-env FLOSSBANK_TEST_SOURCE=1 DEBUG=flossbank nyc --reporter=cobertura npm run --silent test:tap > TEST_RESULTS.xml"
	},
	"nyc": {
		"exclude": [
			"test/**/*.js",
			"ci/tests/**/*.js",
			"webpack.config.js"
		]
	},
	"dependencies": {
		"update-notifier": "^4.0.0"
	},
	"devDependencies": {
		"abort-controller": "^3.0.0",
		"ava": "^3.5.0",
		"boxen": "^4.1.0",
		"chalk": "^2.4.2",
		"ci-info": "^2.0.0",
		"conf": "^6.2.0",
		"cross-env": "^7.0.2",
		"debug": "^4.1.1",
		"diffy": "^2.1.0",
		"husky": "^3.0.5",
		"ls": "^0.2.1",
		"make-dir": "^3.0.2",
		"minimist": "^1.2.0",
		"multi-tap": "^1.0.1",
		"nock": "^11.7.2",
		"node-fetch": "^2.6.0",
		"nopt": "^4.0.3",
		"npm": "^6.14.4",
		"nyc": "^14.1.1",
		"prompts": "^2.2.1",
		"rimraf": "^3.0.2",
		"sinon": "^7.5.0",
		"standard": "^13.1.0",
		"tap-xunit": "^2.4.1",
		"temp-write": "^4.0.0",
		"term-size": "^2.1.0",
		"terser-webpack-plugin": "^2.3.2",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10",
		"word-wrap": "^1.2.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/flossbank/cli.git"
	}
}
